      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sec-ant/zxing-wasm-demo.git"
  },
  "homepage": "https://github.com/Sec-ant/zxing-wasm-demo",
